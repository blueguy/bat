[extractconfig]
configtype = global
database = /tmp/master/master.sqlite3
#authdatabase = /tmp/master/master.sqlite3
#authcopy = string:function:variable
scanlicense = no
licensedb = /tmp/master/licenses.sqlite3
ninkacommentsdb = /tmp/master/ninkacomments.sqlite3
scancopyright = no
#authlicensedb = /tmp/master/authlicenses.sqlite3
securitydb = /tmp/master/security.sqlite3
scansecurity = no
cleanup = yes
wipe = no
unpackdir = /ramdisk
extrahashes = md5:sha1:crc32:tlsh
nomoschunks = 10
urlcutoff = 1000
## below are for for generatelistrpm.py
patchesdir = /tmp/patches
rpmdb = /tmp/rpmdb.sqlite3
insecurerpm = yes
## cutoff is 200 MiB
cutoff = 209715200

## now follows per package configuration

[bash]
configtype = package
extensions = .def:C

[chromium]
configtype = package
blacklist = icudt46l_dat.S:icudt42l_dat.S:icudtl_dat.S:icudt42l_dat.s

[freecad]
configtype = package
blacklist = Arch_rc.py

[linux]
configtype = package
alwaysscan = string:function
